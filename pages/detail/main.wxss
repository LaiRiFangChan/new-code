.bigbox {
  margin-bottom: 100rpx;
}
.lb {
  width: 100%;
  height: 720rpx;
}
.lb image {
  width: 100%;
  height: 720rpx;
}
.msg {
  width: 100%;
  height: 300rpx;
  padding: 0 15rpx;
  box-sizing: border-box;
  border-bottom: 20rpx solid #f4f4f4;
}
.msg .price {
  color: #eb4450;
  font-size: 48rpx;
  margin-top: 35rpx;
}
.msg .word {
  margin-top: 40rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.msg .word .left {
  flex: 1;
  padding-right: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.msg .word .right {
  width: 140rpx;
  text-align: center;
  height: 80rpx;
  border-left: 2rpx solid #ccc;
  position: relative;
}
.msg .word .right ._button {
  width: 140rpx;
  height: 80rpx;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
}
.msg .isSend {
  font-size: 28rpx;
  color: #aaa;
  margin-top: 40rpx;
}
.chuxiao {
  width: 100%;
  height: 196rpx;
  padding: 0 15rpx;
  box-sizing: border-box;
  border-bottom: 20rpx solid #f4f4f4;
}
.chuxiao .top {
  line-height: 98rpx;
}
.chuxiao .top ._span {
  margin-left: 40rpx;
}
.chuxiao .top ._span.color {
  color: #ff2d4a;
}
.tab {
  width: 100%;
  padding: 0 10rpx;
  box-sizing: border-box;
  height: 100rpx;
  display: flex;
  flex-wrap: nowrap;
  text-align: center;
  line-height: 100rpx;
}
.tab .item {
  flex: 1;
}
.tab .item.active {
  color: #ff2d4a;
  border-bottom: 6rpx solid #ff2d4a;
}
.content {
  padding: 20rpx 20rpx;
}
.content .item {
  margin-top: -4rpx;
  width: 100%;
  box-sizing: border-box;
  border: 2rpx solid #aaa;
  display: flex;
  text-align: center;
  height: 85rpx;
  line-height: 85rpx;
}
.content .item .key {
  flex: 1;
  border: 2rpx solid #aaa;
}
.content .item .value {
  flex: 1;
  border: 2rpx solid #aaa;
}
.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  display: flex;
  flex-wrap: nowrap;
  background-color: #fff;
}
.bottom .start {
  flex: 2;
  font-size: 24rpx;
  text-align: center;
  padding-top: 20rpx;
  color: #bbbbbb;
}
.bottom .start.kefu {
  position: relative;
}
.bottom .start.kefu ._button {
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
}
.bottom .end {
  flex: 3;
  font-size: 32rpx;
  color: #fff;
  border-radius: 0;
  line-height: 98rpx;
  text-align: center;
}
.bottom .end.one {
  background-color: #f4b73f;
}
.bottom .end.two {
  background-color: #ff2d4a;
}